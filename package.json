{
  "name": "executive-order-tracker",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "prisma generate",
    "scrape": "tsx src/lib/scraper/index.ts",
    "scrape:watch": "tsx watch src/lib/scraper/index.ts",
    "scheduler": "tsx src/lib/scheduler/index.ts",
    "scheduler:prod": "node dist/lib/scheduler/index.js",
    "scheduler:watch": "tsx watch src/lib/scheduler/index.ts",
    "analyze": "ANALYZE=true next build",
    "type-check": "tsc --noEmit",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,json}\"",
    "validate": "npm-run-all --parallel type-check lint test",
    "prepare": "husky install",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "test": "jest",
    "test:watch": "jest --watch",
    "clean": "rimraf .next out dist",
    "build:scheduler": "tsc -p tsconfig.scheduler.json",
    "pm2:start": "pm2 start npm --name 'eo-scheduler' -- run scheduler:prod",
    "pm2:stop": "pm2 stop eo-scheduler",
    "pm2:logs": "pm2 logs eo-scheduler",
    "test-db": "tsx src/scripts/test-db.ts",
    "test-scraper": "tsx src/scripts/test-scraper.ts",
    "test-api": "tsx src/scripts/test-api.ts",
    "test-scheduler": "tsx src/scripts/test-api.ts -- --check-scheduler"
  },
  "dependencies": {
    "@prisma/client": "^5.10.0",
    "@radix-ui/react-collapsible": "^1.1.3",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-slot": "^1.1.2",
    "axios": "^1.7.9",
    "cheerio": "^1.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^3.3.1",
    "dotenv": "^16.4.1",
    "lucide-react": "^0.323.0",
    "next": "14.1.0",
    "next-themes": "^0.2.1",
    "node-cron": "^3.0.3",
    "node-fetch": "^3.3.2",
    "pino": "^9.6.0",
    "pino-pretty": "^10.3.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "recharts": "^2.12.0",
    "sanitize-html": "^2.11.0",
    "tailwind-merge": "^2.2.1",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.17",
    "@types/node-cron": "^3.0.11",
    "@types/pino": "^7.0.4",
    "@types/pino-pretty": "^5.0.0",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@types/sanitize-html": "^2.9.5",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "autoprefixer": "^10.4.17",
    "critters": "^0.0.20",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.1.0",
    "husky": "^9.0.10",
    "jest": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.35",
    "postcss-import": "^16.0.0",
    "prettier": "^3.2.5",
    "prisma": "^5.10.0",
    "rimraf": "^5.0.5",
    "tailwindcss": "^3.4.1",
    "tailwindcss-animate": "^1.0.7",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}