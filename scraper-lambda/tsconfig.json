{
  "compilerOptions": {
    "target": "ES2022",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": "dist",
    "typeRoots": ["./node_modules/@types", "./"],
    "types": ["node", "aws-lambda"]
  },
  "include": ["*.ts", "custom.d.ts"],
  "exclude": ["node_modules", "dist"]
}