{
  "name": "executive-order-scraper-lambda",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "build": "esbuild index.ts --bundle --minify --platform=node --target=node18 --format=esm --external:@aws-sdk/* --external:@sparticuz/chromium --external:puppeteer-core --outfile=dist/index.mjs",
    "dev": "NODE_OPTIONS=\"--loader ts-node/esm\" node src/scraper/local-scraper.ts",
    "scrape:local": "NODE_OPTIONS=\"--loader ts-node/esm\" node src/scraper/local-scraper.ts",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --config jest.config.ts",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "deploy": "sam deploy",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.504.0",
    "@aws-sdk/credential-providers": "^3.749.0",
    "@aws-sdk/lib-dynamodb": "^3.504.0",
    "@aws-sdk/protocol-http": "^3.370.0",
    "@aws-sdk/signature-v4": "^3.370.0",
    "@prisma/client": "^6.3.1",
    "@radix-ui/react-collapsible": "^1.1.3",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-slot": "^1.1.2",
    "@sparticuz/chromium": "^121.0.0",
    "@tanstack/react-query": "^5.66.0",
    "@tanstack/react-query-devtools": "^5.66.0",
    "axios": "^1.6.7",
    "axios-retry": "^4.0.0",
    "cheerio": "^1.0.0-rc.12",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^3.3.1",
    "dotenv": "^16.4.7",
    "http-status-codes": "^2.3.0",
    "lucide-react": "^0.475.0",
    "next": "^14.1.0",
    "next-themes": "^0.4.4",
    "puppeteer-core": "^24.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sanitize-html": "^2.14.0",
    "tailwind-merge": "^3.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@aws-sdk/types": "^3.504.0",
    "@swc/core": "^1.3.107",
    "@types/aws-lambda": "^8.10.133",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.16",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@types/sanitize-html": "^2.13.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "autoprefixer": "^10.4.17",
    "cross-env": "^7.0.3",
    "esbuild": "^0.20.0",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.1.0",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "postcss": "^8.4.33",
    "prettier": "^3.2.5",
    "prisma": "^6.3.1",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
