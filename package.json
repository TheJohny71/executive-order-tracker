{
  "name": "executive-order-scraper-lambda",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "build": "esbuild index.ts --bundle --minify --platform=node --target=node18 --format=esm --external:@aws-sdk/* --external:@sparticuz/chromium --external:puppeteer-core --outfile=dist/index.mjs",
    "dev": "NODE_OPTIONS=\"--loader ts-node/esm\" node src/scraper/local-scraper.ts",
    "scrape:local": "NODE_OPTIONS=\"--loader ts-node/esm\" node src/scraper/local-scraper.ts",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "deploy": "sam deploy"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.504.0",
    "@aws-sdk/lib-dynamodb": "^3.504.0",
    "@sparticuz/chromium": "^121.0.0",
    "axios": "^1.6.7",
    "axios-retry": "^4.0.0",
    "cheerio": "^1.0.0-rc.12",
    "date-fns": "^3.3.1",
    "http-status-codes": "^2.3.0",
    "next": "^14.1.0",
    "puppeteer-core": "^24.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@aws-sdk/types": "^3.504.0",
    "@swc/core": "^1.3.107",
    "@types/aws-lambda": "^8.10.133",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.16",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "autoprefixer": "^10.4.17",
    "cross-env": "^7.0.3",
    "esbuild": "^0.20.0",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.1.0",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "postcss": "^8.4.33",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "jest": {
    "preset": "ts-jest/presets/default-esm",
    "testEnvironment": "node",
    "extensionsToTreatAsEsm": [".ts"],
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1",
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "transform": {
      "^.+\\.tsx?$": ["ts-jest", {
        "useESM": true,
        "tsconfig": {
          "jsx": "react-jsx"
        }
      }]
    },
    "modulePathIgnorePatterns": [
      "<rootDir>/dist/",
      "<rootDir>/scraper-lambda/deploy/",
      "<rootDir>/scraper-lambda/temp_deploy/",
      "<rootDir>/.next/"
    ]
  }
}