{
  "name": "executive-order-tracker",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "prisma generate",
    "scrape": "tsx src/lib/scraper/index.ts",
    "scrape:watch": "tsx watch src/lib/scraper/index.ts",
    "scheduler": "tsx src/lib/scheduler/index.ts",
    "scheduler:prod": "node dist/lib/scheduler/index.js",
    "scheduler:watch": "tsx watch src/lib/scheduler/index.ts",
    "analyze": "ANALYZE=true next build",
    "type-check": "tsc --noEmit",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "validate": "npm run lint && npm run type-check",
    "prepare": "husky install",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "test": "jest",
    "test:watch": "jest --watch",
    "clean": "rimraf .next out",
    "build:scheduler": "tsc -p tsconfig.scheduler.json",
    "pm2:start": "pm2 start npm --name 'eo-scheduler' -- run scheduler:prod",
    "pm2:stop": "pm2 stop eo-scheduler",
    "pm2:logs": "pm2 logs eo-scheduler"
  },
  "dependencies": {
    "@prisma/client": "^6.3.1",
    "@radix-ui/react-collapsible": "^1.1.3",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-slot": "^1.1.2",
    "axios": "^1.7.9",
    "cheerio": "^1.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^3.0.0",
    "lucide-react": "^0.475.0",
    "next": "^14.2.23",
    "node-cron": "^3.0.3",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "recharts": "^2.15.1",
    "sanitize-html": "^2.14.0",
    "tailwind-merge": "^3.0.1",
    "tailwindcss-animate": "^1.0.7",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/node-cron": "^3.0.11",
    "@types/pino": "^7.0.4",
    "@types/pino-pretty": "^4.7.5",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/sanitize-html": "^2.13.0",
    "@types/tailwindcss": "^3.0.11",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.0.4",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "pm2": "^5.3.0",
    "postcss": "^8.4.31",
    "postcss-import": "^15.1.0",
    "postcss-nesting": "^12.0.1",
    "prettier": "^3.1.0",
    "prisma": "^6.3.1",
    "rimraf": "^5.0.0",
    "tailwindcss": "^3.3.5",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0"
  }
}