{
  "name": "executive-order-tracker",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "prisma generate",
    "scrape": "tsx src/lib/scraper/index.ts",
    "scrape:watch": "tsx watch src/lib/scraper/index.ts",
    "scheduler": "tsx src/lib/scheduler/index.ts",
    "scheduler:prod": "node dist/lib/scheduler/index.js",
    "scheduler:watch": "tsx watch src/lib/scheduler/index.ts",
    "analyze": "ANALYZE=true next build",
    "type-check": "tsc --noEmit",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "validate": "npm run lint && npm run type-check",
    "prepare": "husky install",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "test": "jest",
    "test:watch": "jest --watch",
    "clean": "rimraf .next out",
    "build:scheduler": "tsc -p tsconfig.scheduler.json",
    "pm2:start": "pm2 start npm --name 'eo-scheduler' -- run scheduler:prod",
    "pm2:stop": "pm2 stop eo-scheduler",
    "pm2:logs": "pm2 logs eo-scheduler"
  },
  "dependencies": {
    "@prisma/client": "^5.8.0",
    "axios": "^1.7.9",
    "cheerio": "^1.0.0",
    "date-fns": "^3.0.0",
    "next": "14.0.4",
    "node-cron": "^3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/node-cron": "^3.0.11",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.0.4",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "pm2": "^5.3.0",
    "prettier": "^3.1.0",
    "prisma": "^5.8.0",
    "rimraf": "^5.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0"
  }
}